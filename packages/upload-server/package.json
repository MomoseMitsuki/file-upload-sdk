{
	"name": "@momosemitsuki/upload-server",
	"version": "1.0.4",
	"description": "",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.esm.js",
			"types": "./dist/index.d.ts"
		},
		"./express": {
			"require": "./dist/express-mid.cjs",
			"import": "./dist/express-mid.esm.js",
			"types": "./dist/middlewares/express-mid.d.ts"
		},
		"./koa": {
			"require": "./dist/koa-mid.cjs",
			"import": "./dist/koa-mid.esm.js",
			"types": "./dist/middlewares/koa-mid.d.ts"
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "MomoseMitsuki",
	"license": "MIT",
	"packageManager": "pnpm@10.22.0",
	"buildOptions": {
		"name": "MomoseMitsukiUploadServer",
		"formats": [
			"esm",
			"cjs"
		],
		"external": [
			"tslib",
			"mongoose",
			"@typegoose/typegoose"
		],
		"entries": {
			"index": "./index.ts",
			"express-mid": "./middlewares/express-mid.ts",
			"koa-mid": "./middlewares/koa-mid.ts"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@momosemitsuki/upload-protocol": "workspace:*",
		"@typegoose/typegoose": "^12.20.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^9.0.0",
		"spark-md5": "^3.0.2",
		"tslib": "^2.8.1",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@types/express": "^5.0.5",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/koa": "^3.0.1",
		"@types/spark-md5": "^3.0.5"
	}
}
